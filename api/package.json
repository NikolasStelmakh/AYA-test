{
  "name": "aya-test-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "node": ">=18.13.0",
  "scripts": {
    "parse:dump": "ts-node src/lib/dump-parser.ts",
    "clean": "rm -rf ./build",
    "build": "npm run clean && npx tsc --project tsconfig.build.json",
    "migrate": "node-pg-migrate",
    "migrate:create": "npm run migrate create",
    "migrate:up": "ts-node src/migrate.ts",
    "db:types:update": "ts-node src/lib/knex/update-db-types.ts",
    "start:prod": "concurrently \"npm run build\" \"node --experimental-specifier-resolution=node build/index.js\"",
    "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q --experimental-specifier-resolution=node build/index.js\"",
    "scheduler": "JOB_LIST=tasksNotifications ts-node scheduler.ts",
    "eslint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "test": "node ../node_modules/.bin/jest"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "nikolasstelmah@ukr.net",
  "license": "ISC",
  "dependencies": {
    "@breejs/ts-worker": "^2.0.0",
    "body-parser": "^1.20.1",
    "bree": "^9.1.3",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "knex-types": "^0.5.0",
    "pg": "^8.11.0",
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.10.1",
    "@types/superagent": "^4.1.18",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
